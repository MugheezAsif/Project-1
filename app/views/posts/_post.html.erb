
<div class="my-2">
  <div id="<%= dom_id post %>">
    <p>
      <strong>Author:</strong>
      <%= Account.find(post.account_id).name %>
    </p>

    <p>
      <strong>Title:</strong>
      <%= post.title %>
    </p>

    <p>
      <strong>Content:</strong>
      <%= post.content %>
    </p>

  </div>
</div>